myontology:Subject a owl:Class ;
    rdfs:label "Subject"@en ;
    osmethodology:natural_language_description "The individual of group who undertakes the activity. [Stan Karanasios]" ;
    osmethodology:related_rabbit_sentence """A Subject is anything that:
  is a kind of Individual or Group;
  undertakes an Activity.""",
        "Every Subject has at least one Motivation.",
        "Every Subject interacts with Community.",
        "Every Subject interacts with at least one Object.",
        "Every Subject uses Tools.",
        "Subject is a concept." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:interactWith ;
            owl:someValuesFrom myontology:Community ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass myontology:Motivation ;
            owl:onProperty myontology:subjectHasMotivation ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass myontology:Object ;
            owl:onProperty myontology:interactWith ],
        [ a owl:Restriction ;
            owl:onProperty myontology:subjectUsesTool ;
            owl:someValuesFrom myontology:Tool ],
        owl:Thing ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Class ;
                        owl:unionOf ( myontology:Group myontology:Person ) ] [ a owl:Restriction ;
                        owl:onProperty myontology:undertake ;
                        owl:someValuesFrom myontology:Activity ] ) ] .

myontology:Activity a owl:Class ;
    rdfs:label "Activity"@en ;
    osmethodology:natural_language_description "It's a human, individual or collective activity driven by a motive. Every activity has rules and norms, division of labour, tools, a subject, object and community and an outcome. [Stan Karanasios]" ;
    osmethodology:related_rabbit_sentence "Activity is a concept.",
        "Every Activity has Division of Labour.",
        "Every Activity has Motivation.",
        "Every Activity has Outcome.",
        "Every Activity has Rules & Norms.",
        "Every Activity has at least one Object.",
        "Every Activity has at least one Subject.",
        "Every Activity has at least one Tool.",
        "Every Activity is defined by a Activity Viewpoint that is viewpoint of Subject." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:hasAction ;
            owl:someValuesFrom myontology:Action ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass myontology:Tool ;
            owl:onProperty myontology:hasTool ],
        [ a owl:Restriction ;
            owl:onProperty myontology:isDefinedBy ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( myontology:Activity_Viewpoint [ a owl:Restriction ;
                                owl:onProperty myontology:isViewpointOf ;
                                owl:someValuesFrom myontology:Subject ] ) ] ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass myontology:Object ;
            owl:onProperty myontology:hasObject ],
        [ a owl:Restriction ;
            owl:onProperty myontology:hasRuleNorm ;
            owl:someValuesFrom myontology:Rule_Norm ],
        [ a owl:Restriction ;
            owl:onProperty myontology:activityHasMotivation ;
            owl:someValuesFrom myontology:Motivation ],
        [ a owl:Restriction ;
            owl:onProperty myontology:hasOutcome ;
            owl:someValuesFrom myontology:Outcome ],
        [ a owl:Restriction ;
            owl:onProperty myontology:hasDivisionOfLabour ;
            owl:someValuesFrom myontology:Division_of_Labour ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass myontology:Subject ;
            owl:onProperty myontology:hasSubject ],
        owl:Thing .

myontology:Outcome a owl:Class ;
    rdfs:label "Outcome"@en ;
    osmethodology:related_rabbit_sentence "Outcome is a concept." ;
