    osmethodology:related_rabbit_sentence "Emotion is a concept.",
        "Emotion usually is related to Action.",
        "Emotion usually is related to Activity.",
        "Emotion usually is related to Operation." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:isEmbeddedIn ;
            owl:someValuesFrom myontology:Motivation ],
        [ a owl:Restriction ;
            owl:onProperty myontology:isEmbeddedIn ;
            owl:someValuesFrom myontology:Activity ],
