myontology:Rule_Norm a owl:Class ;
    rdfs:label "Conventions"@en,
        "Norm"@en,
        "Norm (social)"@en,
        "Norms"@en,
        "Regulations"@en,
        "Regulations, Norms and Conventions"@en,
        "Rules & Norms"@en ;
    osmethodology:natural_language_description "Rules & Norms govern Community, Subject and Object. [Stan Karanasios]",
        "Regulations, Norms and Conventions (explicit and implicit) that constrain activity. [Engestr√∂m, Miettinnen et al. 1999]"@en ;
    osmethodology:related_rabbit_sentence "Every Rules & Norms govern Community.",
        "Every Rules & Norms govern Object.",
        "Every Rules & Norms govern Subject.",
        "Rules & Norms is a concept." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:ruleNormGovern ;
            owl:someValuesFrom myontology:Community ],
        [ a owl:Restriction ;
            owl:onProperty myontology:ruleNormGovern ;
            owl:someValuesFrom myontology:Object ],
        [ a owl:Restriction ;
            owl:onProperty myontology:ruleNormGovern ;
            owl:someValuesFrom myontology:Subject ],
        owl:Thing .

