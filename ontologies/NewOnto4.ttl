        "Motivation usually has an Emotion." ;
    rdfs:subClassOf owl:Thing .

myontology:Community a owl:Class ;
    rdfs:label "Community"@en ;
    osmethodology:natural_language_description "Community influences Subject and Object. [Stan Karanasios]",
        "Individuals or groups other than the subject who have some general objects and with whom the subject interacts.  [Engestr√∂m, Miettinnen et al. 1999]" ;
    osmethodology:related_rabbit_sentence "Community and Subject are mutually exclusive.",
        "Community is a concept.",
        "Every Community influences Object.",
        "Every Community influences Subject.",
        "Every Community is a kind of Individual or Group." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:communityInfluence ;
            owl:someValuesFrom myontology:Object ],
        [ a owl:Restriction ;
            owl:onProperty myontology:communityInfluence ;
            owl:someValuesFrom myontology:Subject ],
        [ a owl:Class ;
            owl:unionOf ( myontology:Group myontology:Person ) ],
        owl:Thing ;
    owl:disjointWith myontology:Subject .

