myontology:Division_of_Labour a owl:Class ;
    rdfs:label "Division of Labour"@en,
        "Hierarchical Structure"@en,
        "Hierarchy"@en,
        "Roles Structure"@en,
        "Task Division"@en ;
    osmethodology:natural_language_description "The way the tasks are divided up and the way roles and hierarchies are structured. Division of Labour is performed by community, subject and object.  [Engestr√∂m, Miettinnen et al. 1999]" ;
    osmethodology:related_rabbit_sentence "Division of Labour is a concept.",
        "Every Division of Labour is performed by Community.",
        "Every Division of Labour is performed by Object.",
        "Every Division of Labour is performed by Subject." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:isPerformedBy ;
            owl:someValuesFrom myontology:Object ],
        [ a owl:Restriction ;
            owl:onProperty myontology:isPerformedBy ;
            owl:someValuesFrom myontology:Subject ],
        [ a owl:Restriction ;
            owl:onProperty myontology:isPerformedBy ;
            owl:someValuesFrom myontology:Community ],
        owl:Thing .

