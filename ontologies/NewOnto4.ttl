myontology:Action a owl:Class ;
    rdfs:label "Action" ;
    osmethodology:natural_language_description "Goal orientated actions that contributes to the attainment of the motive for the activity.  [Engestr√∂m, Miettinnen et al. 1999]" ;
    osmethodology:related_rabbit_sentence "Action is a concept.",
        "Every Action has Goal.",
        "Every Action has Operation.",
        "Every Action is part of Activity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty myontology:hasOperation ;
            owl:someValuesFrom myontology:Operation ],
        [ a owl:Restriction ;
            owl:onProperty myontology:isPartOfActivity ;
            owl:someValuesFrom myontology:Activity ],
        [ a owl:Restriction ;
            owl:onProperty myontology:hasGoal ;
            owl:someValuesFrom myontology:Goal ],
        owl:Thing .

